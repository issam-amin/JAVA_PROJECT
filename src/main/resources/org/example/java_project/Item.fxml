<?import javafx.scene.layout.HBox?>

<HBox fx:id="itemC" stylesheets="@Style/barChart.css" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="53.0" prefWidth="895.0" spacing="80.0"   xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fitHeight="31.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/icons8_GPS_Antenna_64px_1.png" />
            </image>
            <HBox.margin>
                <Insets left="10.0" />
            </HBox.margin>
        </ImageView>
        <Label fx:id="id" prefHeight="17.0" prefWidth="117.0" text="KeepToo" textFill="#b7c3d7" />
        <Label fx:id="count" prefHeight="17.0" prefWidth="85.0" text="JFK" textFill="#b7c3d7">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Label>
        <Label fx:id="countDate" text="10/11/2018" textFill="#b7c3d7">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Label>
        <Label fx:id="percentage" prefHeight="17.0" prefWidth="33.0" text="\$200" textFill="#b7c3d7">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Label>
        <Button fx:id="ActionButton" mnemonicParsing="false" prefHeight="10.0" prefWidth="60.0"  text="Show">
